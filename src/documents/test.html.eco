<div class="row">

  <div class="span4">
    <a href="#" id="linktest">Event Based Send</a>
  </div>

  <div class="span4">
    <h2>Jstag in iframe</h2>
    <iframe src="iframetestbasic.html" width="100%" height="500px"></iframe>
  </div>

  <div class="span4">
    <h2>Debug Output</h2>
    <div id="output"></div>
  </div>

</div>

<!-- start jstag  -->
<script type="text/javascript">
  window.jstag=function(e){var t={_q:[],_c:e,ts:(new Date).getTime()},n=!1,r=window,i=document,s="/static/io",o=Array.prototype.slice,u=e.js?e.js:e.url||"";return e.ext=e.ext||".min.js",t.load=function(){var t,o=i.getElementsByTagName("script")[0];return n=!0,"JSON"in r&&Array.prototype.forEach||(s+="w"),i.getElementById(s)?this:(t=i.createElement("script"),t.id=s,t.src=u+s+e.ext,o.parentNode.insertBefore(t,o),this)},t.bind=function(e){this._q.push([e,o.call(arguments,1)])},t.ready=function(){this._q.push(["ready",o.call(arguments)])},t.send=function(){return n||this.load(),this._q.push(["ready","send",o.call(arguments)]),this},r.jstag=t,t
